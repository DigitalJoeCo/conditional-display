{
    "name": "digitaldyve/conditional-display",
    "description": "Allows you to load scripts, sidebars, and more conditionally. Any way you could want.",
    "type": "wordpress-plugin",
    "require": {
        "htmlburger/carbon-fields": "^3.6",
        "htmlburger/carbon-field-number": "^3.0",
        "composer/installers": "^1.0 || ^2.0"
    },
    "license": "GPL",
    "autoload": {
        "psr-4": {
            "DigitalDyve\\ConditionalDisplay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Digital Dyve"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "composer/installers": true
        },
        "vendor-dir": "vendor"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:wordpress-plugin"]
        }
    }
}
