{
    "name": "digitaldyve/conditional-display",
    "description": "Allows you to load scripts, sidebars, and more conditionally. Any way you could want.",
    "type": "wordpress-plugin",
    "require": {
        "htmlburger/carbon-fields": "^3.6",
        "htmlburger/carbon-field-number": "^3.0",
        "composer/installers": "^1.0 || ^2.0"
    },
    "license": "GPL",
    "autoload": {
        "psr-4": {
            "DigitalDyve\\ConditionalDisplay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Digital Dyve"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist",
        "vendor-dir": "./vendor",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "@php set-install-path.php"
        ],
        "pre-update-cmd": [
            "@php set-install-path.php"
        ]
    },
    "extra": {
        "installer-paths": {
            "{$CONDITIONAL_DISPLAY_INSTALL_PATH}/vendor/{$vendor}/{$name}": ["htmlburger/carbon-fields", "htmlburger/carbon-field-number"]
        }
    }
}
