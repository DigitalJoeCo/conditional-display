{
    "name": "digitaldyve/conditional-display",
    "description": "Allows you to load scripts, sidebars, and more conditionally. Any way you could want.",
    "type": "wordpress-plugin",
    "require": {
        "mnsami/composer-custom-directory-installer": "^2.0",
        "htmlburger/carbon-fields": "^3.6",
        "htmlburger/carbon-field-number": "^3.0"
    },
    "license": "GPL",
    "autoload": {
        "psr-4": {
            "DigitalDyve\\ConditionalDisplay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Digital Dyve"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "preferred-install": "dist",
        "vendor-dir": "./vendor",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/installers": true,
            "mnsami/composer-custom-directory-installer": true
        }
    },
    "extra": {
        "installer-paths": {
            "./vendor/{$vendor}/{$name}": [
                "htmlburger/carbon-fields", 
                "htmlburger/carbon-field-number"
            ]
        }
    }
}
