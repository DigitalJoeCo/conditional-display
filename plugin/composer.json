{
    "name": "digitaldyve/conditional-display",
    "description": "Allows you to load scripts, sidebars, and more conditionally. Any way you could want.",
    "type": "wordpress-plugin",
    "license": "GPL",
    "autoload": {
        "psr-4": {
            "DigitalDyve\\ConditionalDisplay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Digital Dyve"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "allow-plugins": {
            "roots/wordpress-core-installer": true
        }
    },
    "require": {
        "htmlburger/carbon-field-number": "^3.0",
        "htmlburger/carbon-fields": "^3.6"
    },
    "require-dev": {
        "roots/wordpress": "^6.6"
    },
    "extra": {
        "wordpress-install-dir": "../wordpress"
    }
}
